@using MsWebGame.CSKH.Utils
@model  MsWebGame.CSKH.Models.Accounts.AdminModel
@{
    //page title
    ViewBag.Title = AppConstants.CONFIG.ADD_NEW;
}
@using (Html.BeginForm())
{
    <div class="section-header">
        <div class="title">
            <img src="@Url.Content("~/Content/images/ico-promotions.png")" alt="" />
            @AppConstants.CONFIG.ADD_NEW @Html.ActionLink("(" + AppConstants.CONFIG.BACK + ")", "Index")
        </div>
        <div class="options">
            <input type="submit" name="save" class="t-button" value="@AppConstants.CONFIG.SAVE" />
            
        </div>
    </div>
    @Html.Partial("_CreateOrUpdate", Model)
}