@using MsWebGame.CSKH.Utils
@model  MsWebGame.CSKH.Database.DTO.BotCheck
@{
    ViewBag.Title = "Kiểm tra số dư bot";
}
    <div class="section-header">
        <div class="title">
            <img src="@Url.Content("~/Content/images/ico-promotions.png")" alt="" />
            Kiểm tra số dư bot
        </div>
    </div>
    <div class="t-widget t-tabstrip t-header">
        <div class="t-content t-state-active" style="display: block;">
            <table class="adminContent customer-info-tab">
                @if (Model != null)
                {
                    <tr>
                        <td class="adminTitle">
                            @Html.LabelFor(model => model.BetNumber):
                        </td>
                        <td class="adminData">
                            @Model.BetNumber
                        </td>
                    </tr>
                    <tr>
                        <td class="adminTitle">
                            @Html.LabelFor(model => model.TotalBet):
                        </td>
                        <td class="adminData">
                            @Model.TotalBet
                        </td>
                    </tr>
                    <tr>
                        <td class="adminTitle">
                            @Html.LabelFor(model => model.TotalPrize):
                        </td>
                        <td class="adminData">
                            @Model.TotalPrize
                        </td>
                    </tr>
                    <tr>
                        <td class="adminTitle">
                            @Html.LabelFor(model => model.delta):
                        </td>
                        <td class="adminData">
                            @Model.delta
                        </td>
                    </tr>
                }
                else
                {
                    <tr>
                        <td class="adminTitle">
                            Không có dữ liệu
                        </td>
                    </tr>
                }

                <tr>
                    <td class="adminTitle"></td>
                    <td class="adminData">
                        <a href="@Url.Action("BotManager")" class="t-button">Quay lại</a>
                    </td>
                </tr>
            </table>
        </div>
    </div>